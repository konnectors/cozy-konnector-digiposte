{
  "name": "cozy-konnector-digiposte",
  "version": "1.0.3",
  "description": "Import your digiposte files",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy/cozy-konnector-digiposte.git"
  },
  "keywords": [],
  "author": "Cozy Cloud",
  "license": "AGPL-3.0",
  "main": "./src/index.js",
  "eslintConfig": {
    "extends": [
      "eslint-config-cozy-app"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "scripts": {
    "start": "node ./index.js",
    "dev": "cozy-run-dev",
    "standalone": "cozy-run-standalone",
    "test": "konitor testit .",
    "check": "konitor check .",
    "build": "webpack",
    "precommit": "yarn lint",
    "lint": "eslint --fix .",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=${DEPLOY_REPOSITORY:-https://$GITHUB_TOKEN@github.com/konnectors/cozy-konnector-digiposte.git}",
    "cozyPublish": "cozy-app-publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build})",
    "travisDeployKey": "./bin/generate_travis_deploy_key"
  },
  "dependencies": {
    "bluebird": "3.5.1",
    "cozy-konnector-libs": "4.5.2",
    "date-fns": "1.29.0"
  },
  "devDependencies": {
    "babel-loader": "7.1.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "copy-webpack-plugin": "4.5.1",
    "cozy-app-publish": "0.3.6",
    "cozy-jobs-cli": "1.2.0",
    "eslint": "4.19.1",
    "eslint-config-cozy-app": "0.8.0",
    "git-directory-deploy": "1.5.1",
    "husky": "0.14.3",
    "konitor": "0.8.3",
    "regenerator-runtime": "0.11.1",
    "webpack": "4.10.2",
    "webpack-cli": "3.0.2"
  }
}
